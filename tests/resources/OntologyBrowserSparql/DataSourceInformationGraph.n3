@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix shk: <http://shk/> .
@prefix smw: <http://smw/> .
@prefix sdprop: <http://smw/sdprop#> .
@prefix void: <http://rdfs.org/ns/void#> .

 smw:DataSource1 rdf:type shk:DataSource;
 	 sdprop:id "Toyota"^^xsd:string ;
	 rdfs:label "Toyota"^^xsd:string ;
	 void:sparqlEndpoint <http://sparql.dpedia.org>;
	 void:dataDumps <http://dumps.dpedia.org>;
	 shk:refreshInterval "1 month" ;
	 shk:mapping smw:dbpediaMapping ;
	 shk:silkIDresolutionRules smw:dbpediaSilkIDResolution .

 smw:DataSource2 rdf:type shk:DataSource;
 	 sdprop:id "Volkswagen"^^xsd:string ;
	 rdfs:label "Volkswagen"^^xsd:string ;
	 void:sparqlEndpoint <http://sparql.mywiki.org>;
	 shk:refreshInterval "1 day" ;
	 shk:mapping smw:mywikiMapping ;
	 shk:silkIDresolutionRules smw:mywikiSilkIDResolution .

